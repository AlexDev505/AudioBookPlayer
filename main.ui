   <property name="styleSheet">
    <string notr="true">/* QFRAME */
QFrame {
	border: none;
}

/*  BUTTONS */
QPushButton {
	border: 0px solid #000;
	border-radius: 5px;	
	background-color: rgba(0, 0, 0, 0);
}
QPushButton:hover {
	border-radius: 5px;	
	background-color: #34373C;
}
QPushButton:pressed {	
	background-color: #37393F;
}

/* LINE EDIT */
QLineEdit {
	background-color: rgb(64, 68, 75);
	border: none;
	border-radius: 5px;
	padding-left:3px;
}

/*  TOOLTIP */
QToolTip {
	background-color: #1D1E22;
	border: none;
	border-left: 2px solid #8E9297;
	text-align: left;
	padding: 4px;
}

/*  MENU */
#menuFrame * {
	text-align: left;
}
#menuFrame QPushButton {
	padding: 6px 10px 6px 10px;
}

/*  CONTENT */
#content, #content * {
	background-color: rgb(54, 57, 63);
}

/* LIBRARY FILTERS PANEL */
#libraryFiltersPanel {
	border-top-left-radius: 15px;
	background-color: rgb(47, 49, 54);
}
#libraryFiltersPanel QFrame {
	background-color: rgb(47, 49, 54);
}
#toggleBooksFilterPanelBtn {
	border-radius: 0px;
	border-top-left-radius: 15px;
	background-color: rgb(54, 57, 63);
}
#toggleBooksFilterPanelBtn:hover {
	background-color: rgb(50, 53, 59);
}

/*  CONTROL PANEL */
#controlPanel, #controlPanel QFrame {
	background-color: rgb(41, 43, 47);
}

/* SLIDER */
QSlider {
	background-color: rgb(41, 43, 47);
}
QSlider::groove:horizontal {
	background: #40444B;
	border-radius: 1px;
}
QSlider::sub-page:horizontal {
	background: #8E9297;
	border-radius: 1px;
	height: 40px;
}
QSlider::add-page:horizontal {
	background: #40444B;
	border-radius: 1px;
	height: 40px;
}
QSlider::handle:horizontal {
	background: #8E9297;
	border: 0px;
    width: 8px;
    margin-top: 0px;
    margin-bottom: 0px;
	border-radius: 1px;
}

/* TAB WIDGET */
QTabWidget::pane {
	border: none;
} 
QTabBar::tab {
	background: rgba(0, 0, 0, 0); 
 	border: none;
	padding: 10px;
	font-weight: normal;
} 
QTabBar::tab:selected {
	border-top: 2px solid #fff;
  	background: rgba(0, 0, 0, 0);
	font-weight: bold;
	margin-bottom: -1px; 
}

/*SCROLLBAR */
QScrollArea {
	border: none;
}
/* VERTICAL SCROLLBAR */
 QScrollBar:vertical {
	border: none;
    width: 8px;
	margin: 15px 0px 15px 0 px;
 }
/*  HANDLE BAR VERTICAL */
QScrollBar::handle:vertical {
	background-color: rgb(32, 34, 37);
	min-height: 30px;
	border-radius: 4px;
}
/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:vertical {
	height: 0px;
}
/* BTN BOTTOM - SCROLLBAR */
QScrollBar::add-line:vertical {
	height: 0px;
}
/* RESET ARROW */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
	background: #2E3338;
	border-radius: 4px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: #2E3338;
	border-radius: 4px;
}

</string>
